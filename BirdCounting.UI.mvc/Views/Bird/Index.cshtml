@model IList<Bird>
@{
    ViewData["Title"] = "Birds";
}

<h1>@ViewData["Title"]</h1>

<a class="btn btn-primary" asp-action="Create">New Bird</a>

<table class="table table-striped table-dark">
    <thead>
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Photo</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var bird in Model)
    {
        <partial name="BirdPartial" model="bird"/>
    }
    </tbody>
</table>
@model Session

<h1>Session Details</h1>

<p>Start Time: @Model.StartTime</p>
<p>End Time: @Model.EndTime</p>
<p>Location: @Model.Location</p>

<h2>Birds and Counts</h2>
<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Photo</th>
            <th>Name</th>
            <th>Description</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var bird in Model.Birds)
        {
            <tr>
                <td><img src="@bird.PhotoUrl" alt="@bird.Name" width="100" height="100" /></td>
                <td>@bird.Name</td>
                <td>@bird.Description</td>
                <td>@bird.Count</td>
            </tr>
        }
    </tbody>
